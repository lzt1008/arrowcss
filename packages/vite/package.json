{
  "name": "@rswind/vite",
  "type": "module",
  "version": "0.0.1-alpha.1",
  "description": "Rswind's Vite Plugin",
  "author": "Neo Lyon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lzt1008/rswind.git"
  },
  "keywords": [
    "tailwind",
    "atomic-css",
    "css",
    "rust",
    "vite"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch --sourcemap inline"
  },
  "dependencies": {
    "rswind": "workspace:^"
  },
  "devDependencies": {
    "rollup": "^4.18.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.3"
  }
}
