{
  "name": "@rswind/cli",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "bin": {
    "rswind": "bin/cli.js"
  },
  "files": [
    "bin",
    "bindings"
  ],
  "scripts": {
    "build": "napi build --cargo-cwd ../.. --platform --cargo-name binding_cli -p binding_cli --release bindings",
    "build:dev": "napi build --cargo-cwd ../.. --platform --cargo-name binding_cli -p binding_cli bindings"
  },
  "napi": {
    "binaryName": "rswind-cli",
    "packageName": "@rolldown/cli-binding",
    "targets": [
      "x86_64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "i686-pc-windows-msvc",
      "armv7-unknown-linux-gnueabihf",
      "aarch64-unknown-linux-gnu",
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc",
      "wasm32-wasip1-threads"
    ]
  },
  "keywords": [],
